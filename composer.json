{
    "name": "ptuchik/billing",
    "description": "Billing package for Laravel 5.5 supporting packages, plans, coupons, addons, payments and subscriptions",
    "keywords": [
        "ptuchik",
        "laravel",
        "billing",
        "purchase",
        "subscriptions",
        "currencies",
        "coupons",
        "discounts",
        "plans"
    ],
    "license": "MIT",
    "type": "library",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Hovakimyannn/core-utilities"
        }
    ],

    "require": {
        "illuminate/support": "^5.5|^5.6|^5.7|^5.8|^6.0|^7.0|^8.0|^9.0|^10.0",
        "ptuchik/core-utilities": "dev-master#2b84e598f6fb7b343b4f1f80010178c23ab24c96",
        "league/omnipay": "^3.0",
        "ptuchik/braintree": "1.1.6",
        "torann/currency": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ptuchik\\Billing\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Avik Aghajanyan",
            "email": "avik.aghajanyan@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Ptuchik\\Billing\\Providers\\BillingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
